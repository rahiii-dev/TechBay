        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th>User name</th>
                <th>Phone</th>
                <th>email</th>
                <th>Signup Mode</th>
                <th>Status</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody>
              <% for(user of locals.users) { %>
                <tr>
                  <td><%= user.name %></td>
                  <td><%= user.mobile_no %></td>
                  <td><%= user.email %></td>
                  <td>
                    <% if(user.googleId){ %>
                        Google
                    <% } else if(user.facebookId){%>
                        Facebook
                    <% } else {%>
                        Normal
                    <% } %>
                  </td>
                  <td>
                    <% if(user.isBlocked == false){%>
                        <span class="bg-lightgreen badges text-white">Active</span>
                    <% } else{%>
                        <span class="bg-lightred badges text-white">Blocked</span>
                    <%}%>  
                 </td>
                  <td>
                    <% if(user.isBlocked == false){%>
                        <a class="btn bg-dark text-white" onclick="confirmDelete('/admin/user/block/<%= user.id %>?_method=PATCH', 'Block User')" href="javascript:void(0);">Block</a>
                    <% } else{%>
                        <a class="btn bg-warning text-white" href="/admin/user/unblock/<%= user.id %>?_method=PATCH">Un Block</a>
                    <%}%>  
                    
                  </td>
                </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
